{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "import",
      "@typescript-eslint"

    ],
    "rules": {
      "no-console": "off",
      "consistent-return": "off"
    }
  }